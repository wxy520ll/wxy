<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="@dimen/space_70"
    android:background="@drawable/tsys_item" >

    <TextView
        android:id="@+id/tv_fjbh"
        android:layout_width="@dimen/space_80"
        android:layout_height="@dimen/space_30"
        android:gravity="center"
        android:textColor="@color/white"
        android:layout_gravity="center" />

    <include layout="@layout/textview_fill_width"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/bg_dark"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_fl1"
            android:layout_width="@dimen/space_0"
            android:layout_weight="1"
            android:layout_height="@dimen/space_30"
            android:layout_marginRight="@dimen/space_5"
            android:layout_marginLeft="@dimen/space_15"
            android:layout_marginTop="@dimen/space_5"
            android:layout_marginBottom="@dimen/space_5"
            android:gravity="center"
            android:text="上午"
            android:textSize="@dimen/text_size_11"
            android:background="@drawable/bg_white_point"
            android:layout_gravity="center" />

        <!--<include layout="@layout/tect_view"/>-->

        <TextView
            android:id="@+id/tv_fl2"
            android:layout_width="@dimen/space_0"
            android:layout_weight="1"
            android:layout_height="@dimen/space_30"
            android:layout_marginRight="@dimen/space_15"
            android:layout_marginLeft="@dimen/space_5"
            android:layout_marginTop="@dimen/space_5"
            android:layout_marginBottom="@dimen/space_5"
            android:gravity="center"
            android:text="下午"
            android:textSize="@dimen/text_size_11"
            android:background="@drawable/bg_white_point"
            android:layout_gravity="center" />



    </LinearLayout>

</LinearLayout>